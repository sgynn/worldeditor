<?xml version="1.0"?>
<gui height="1024" width="1024">
	<external file="data/gui.xml"/>
	<!--Templates-->
	<template rect="0 0 100 100" name="frame" skin="panel" category="Containers"/>
	<template category="Containers" class="SplitPane" rect="0 0 100 100" name="vsplit" skin="none" orientation="vertical">
		<widget anchor="lr" rect="0 46 100 2" name="_sash" skin="button"/>
	</template>
	<template class="Checkbox" rect="0 0 16 16" name="checkbox" skin="none" category="Input">
		<widget anchor="lm" inheritstate="1" tangible="0" rect="0 0 16 16" name="_box" skin="panel"/>
		<widget anchor="lrm" class="Label" inheritstate="1" rect="20 0 12 0" skin="none" name="_text" tangible="1"/>
	</template>
	<template class="TreeView" rect="0 0 100 100" name="tree" skin="panel" category="Lists">
		<widget anchor="tlrb" rect="1 1 86 98" name="_client" skin="none"/>
		<widget anchor="trb" template="vscroll" name="_scroll" rect="87 0 13 100"/>
		<widget rect="0 6 8 8" name="_expand" skin="panel"/>
		<widget class="Label" rect="0 0 100 20" name="_item" skin="listitem"/>
	</template>
	<template class="Textbox" name="editbox" skin="panel" category="Input"/>
	<template category="Display" class="Label" name="label" skin="panel" caption="Label"/>
	<!--Widget Layout-->
	<widget rect="168 192 276 600" template="dialogfixed" name="objecteditor" caption="Object Editor" skin="default">
		<widget anchor="lrtb" template="vsplit" rect="0 0 266 573">
			<widget class="Widget" template="frame" rect="0 0 266 298">
				<widget anchor="lrtb" hideroot="1" template="tree" name="tree" rect="0 0 266 276">
					<widget anchor="lr" class="Widget" visible="0" template="frame" rect="38 66 178 24" skin="none" name="_item" tangible="2">
						<widget inheritstate="1" anchor="lrtb" template="label" name="_0" skin="listitem" rect="0 0 144 24"/>
						<widget anchor="r" template="checkbox" name="_4" rect="155 3 16 16" drag="1"/>
					</widget>
				</widget>
				<widget anchor="lrb" template="editbox" name="property" rect="72 276 192 20"/>
				<widget anchor="lb" template="label" caption="Property" rect="0 276 42 20"/>
			</widget>
			<widget class="Widget" template="frame" rect="0 300 266 273">
				<widget anchor="lrtb" hideroot="1" template="tree" name="resources" rect="0 0 266 250"/>
				<widget anchor="lrb" template="editbox" name="path" rect="0 251 264 20"/>
			</widget>
		</widget>
	</widget>
</gui>
